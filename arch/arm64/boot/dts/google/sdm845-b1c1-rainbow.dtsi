&qupv3_se5_spi {
	status = "disabled";
};

&qupv3_se5_i2c {
	/* VD6281 */
	status = "ok";
	vd6281: vd6281@20 {
		compatible = "st,vd6281";
		reg = <0x20>;
		vdd-supply = <&pm8998_lvs1>;
		regulator-names = "vdd";
		status = "ok";
	};
};

